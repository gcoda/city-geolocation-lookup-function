  "description": "",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "start": "node dist/test.js",
    "build": "tsc",
    "prebuild": "rimraf dist",
    "postbuild": "cp -r src/generated dist/",
    "build:watch": "tsc --watch",
